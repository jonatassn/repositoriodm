              android:supportsRtl="true"
              android:theme="@style/AppTheme"
              tools:ignore="GoogleAppIndexingWarning">
            <activity android:name=".MainActivity">
                  <intent-filter>
                        <action android:name="android.intent.action.MAIN"/>

                        <category android:name="android.intent.category.LAUNCHER"/>
                  </intent-filter>
            </activity>
            <activity android:name=".PerfilRagnarActivity"/>
            <activity android:name=".PerfilLagerthaActivity"/>
            <activity android:name=".PerfilBjornActivity"/>
            <activity android:name=".PerfilRolloActivity"/>
            <activity android:name=".PerfilAthelstanActivity"/>
            <activity android:name=".PerfilFlokiActivity"/>
      </application>

</manifest>